 <script type="text/javascript">
function submitForm(){
	
		
		var hotel_id = document.getElementById("hotel").value;
		var checkinm = document.getElementById("checkin").value;
		var checkoutm = document.getElementById("checkout").value;
		if(hotel_id == '' || hotel_id == null)
		{
			alert('Please select Hotel first!');
	      	return false;
		}
		if(checkinm == '' && checkoutm == '')
	    {
	      alert('Checkin/Checkout Dates are mandatory');
	      return false;
	    }
		var sPath = "https://gc.synxis.com/rez.aspx?hotel=" + hotel_id + "&arrive=" + checkinm + "&depart=" + checkoutm + "&promo=&start=availresults&search=Book+Now";
        var new_sPath ="https://secure.staah.com/common-cgi/package/packagebooking.pl?propertyId=7269&checkIn=" + checkinm + "&checkOut=" + checkoutm + "&location=&noofrooms=1&adult0=1&child0=0&ccode=&unk=2";
        var new_sPath2 ="https://secure.staah.com/common-cgi/package/packagebooking.pl?propertyId=7996&checkIn=" + checkinm + "&checkOut=" + checkoutm + "&location=&noofrooms=1&adult0=1&child0=0&ccode=&unk=2";
        var new_sPath3 ="https://secure.staah.com/common-cgi/package/packagebooking.pl?propertyId=8982&checkIn=" + checkinm + "&checkOut=" + checkoutm + "&location=&noofrooms=1&adult0=1&child0=0&ccode=&unk=2";
    
    if(hotel_id == '7269'){
        window.open(new_sPath, "_blank");
        
    }
    else if(hotel_id == '7996'){
        window.open(new_sPath2, "_blank");
        
    }
    else if(hotel_id == '8982'){
        window.open(new_sPath3, "_blank");
        
    }
    else{
        window.open(sPath, "_blank");
    }
    return false;
    
}
</script>



